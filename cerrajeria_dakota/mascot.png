<svg xmlns="http://www.w3.org/2000/svg" viewBox="0 0 400 500">
  <!-- Key Character Mascot -->
  
  <!-- Key Body (Shaft) -->
  <rect x="180" y="200" width="40" height="180" fill="#C0C0C0" stroke="#999" stroke-width="3" rx="5"/>
  
  <!-- Key Head -->
  <circle cx="200" cy="150" r="60" fill="#D3D3D3" stroke="#999" stroke-width="3"/>
  <circle cx="200" cy="150" r="45" fill="none" stroke="#999" stroke-width="2"/>
  <circle cx="200" cy="150" r="25" fill="#FFD700"/>
  
  <!-- Face -->
  <!-- Eyes -->
  <ellipse cx="185" cy="140" rx="8" ry="12" fill="#333"/>
  <ellipse cx="215" cy="140" rx="8" ry="12" fill="#333"/>
  <circle cx="187" cy="138" r="3" fill="white"/>
  <circle cx="217" cy="138" r="3" fill="white"/>
  
  <!-- Smile -->
  <path d="M 180 160 Q 200 170 220 160" fill="none" stroke="#333" stroke-width="3" stroke-linecap="round"/>
  
  <!-- Cap -->
  <ellipse cx="200" cy="100" rx="50" ry="15" fill="#8B0000"/>
  <path d="M 150 100 Q 150 80 200 75 Q 250 80 250 100" fill="#8B0000"/>
  <circle cx="200" cy="75" r="8" fill="#FFD700"/>
  
  <!-- Left Arm (Thumbs Up) -->
  <g transform="translate(140, 220) rotate(-20)">
    <rect x="0" y="0" width="15" height="50" fill="#FFD700" rx="7"/>
    <!-- Thumb -->
    <rect x="-5" y="10" width="12" height="20" fill="#FFD700" rx="6" transform="rotate(-60 0 10)"/>
    <!-- Glove cuff -->
    <rect x="-2" y="48" width="19" height="8" fill="#FF6B35" rx="2"/>
  </g>
  
  <!-- Right Arm (Thumbs Up) -->
  <g transform="translate(245, 220) rotate(20)">
    <rect x="0" y="0" width="15" height="50" fill="#FFD700" rx="7"/>
    <!-- Thumb -->
    <rect x="8" y="10" width="12" height="20" fill="#FFD700" rx="6" transform="rotate(60 8 10)"/>
    <!-- Glove cuff -->
    <rect x="-2" y="48" width="19" height="8" fill="#FF6B35" rx="2"/>
  </g>
  
  <!-- Key Teeth -->
  <rect x="220" y="340" width="20" height="12" fill="#999"/>
  <rect x="220" y="355" width="20" height="12" fill="#999"/>
  <rect x="220" y="370" width="20" height="8" fill="#999"/>
  
  <!-- Shine effect on key head -->
  <ellipse cx="180" cy="130" rx="15" ry="20" fill="white" opacity="0.3"/>
  
  <!-- Belt/Detail -->
  <rect x="175" y="195" width="50" height="12" fill="#8B0000" rx="3"/>
  <circle cx="200" cy="201" r="5" fill="#FFD700"/>
</svg>
